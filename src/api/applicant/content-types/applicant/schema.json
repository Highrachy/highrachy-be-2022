{
  "kind": "collectionType",
  "collectionName": "applicants",
  "info": {
    "singularName": "applicant",
    "pluralName": "applicants",
    "displayName": "Applicant",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true,
      "maxLength": 150
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true,
      "maxLength": 20
    },
    "resume": {
      "type": "string",
      "required": true
    },
    "job": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::job.job",
      "inversedBy": "applicants"
    },
    "reviewed": {
      "type": "boolean",
      "default": false
    }
  }
}
